<?xml version="1.0" encoding="UTF-8"?>
<AUTOSAR xmlns="http://autosar.org/schema/r4.2" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://autosar.org/schema/r4.2 AUTOSAR_4-2-0.xsd">
  <AR-PACKAGES>
    <AR-PACKAGE>
      <SHORT-NAME>ECUCPackage</SHORT-NAME>
      <LONG-NAME>ECUC Configuration Package</LONG-NAME>
      <ELEMENTS>
        <ECUC-VALUE-COLLECTION>
          <SHORT-NAME>EcuC</SHORT-NAME>
          <LONG-NAME>ECU Configuration</LONG-NAME>
          <DEFINITION-REF>/AUTOSAR/EcuC</DEFINITION-REF>
          <CONTAINERS>
            <ECUC-PARAM-CONF-CONTAINER-VALUE>
              <SHORT-NAME>EcuCGeneral</SHORT-NAME>
              <LONG-NAME>ECU General Configuration</LONG-NAME>
              <DEFINITION-REF>/AUTOSAR/EcuC/EcuCGeneral</DEFINITION-REF>
              <PARAMETERS>
                <ECUC-TEXTUAL-PARAM-VALUE>
                  <DEFINITION-REF>/AUTOSAR/EcuC/EcuCGeneral/EcuCName</DEFINITION-REF>
                  <VALUE>SampleECU</VALUE>
                </ECUC-TEXTUAL-PARAM-VALUE>
                <ECUC-NUMERICAL-PARAM-VALUE>
                  <DEFINITION-REF>/AUTOSAR/EcuC/EcuCGeneral/EcuCMainFunctionPeriod</DEFINITION-REF>
                  <VALUE>10</VALUE>
                </ECUC-NUMERICAL-PARAM-VALUE>
                <ECUC-BOOLEAN-PARAM-VALUE>
                  <DEFINITION-REF>/AUTOSAR/EcuC/EcuCGeneral/EcuCEnableWakeupSource</DEFINITION-REF>
                  <VALUE>true</VALUE>
                </ECUC-BOOLEAN-PARAM-VALUE>
              </PARAMETERS>
            </ECUC-PARAM-CONF-CONTAINER-VALUE>
          </CONTAINERS>
        </ECUC-VALUE-COLLECTION>
      </ELEMENTS>
    </AR-PACKAGE>
  </AR-PACKAGES>
</AUTOSAR>